  "scripts": {
    "watch:client": "parcel watch -t browser web-client/index.html --public-url /static --out-dir dist/static --hmr-port 44412 --no-cache",
    "build:client:prod": "parcel build -t browser web-client/index.html --public-url /static --out-dir dist/static --no-source-maps",
    "watch:server": "watcher",
    "dev": "run-p watch:client watch:server",
    "build:server:prod:linux": "statik -src=`pwd`/dist/static &&  GOARCH=amd64 GOOS=linux CGO_ENABLED=1 go build -ldflags \"-linkmode external -extldflags -static\" main.go",
    "build:prod": "run-s build:client:prod build:server:prod:linux",
    "deploy": "./project/script/deploy.sh",
    "test": "go env"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@wufe/floor-animation": "1.0.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-particles-js": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "sass": "^1.23.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "foundation-sites": "^6.5.3",
    "nord": "^0.2.1"
  }
}
